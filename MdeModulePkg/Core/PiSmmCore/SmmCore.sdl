End

INFComponent
    Name  = "PiSmmIpl"
    File  = "PiSmmIpl.inf"
    Package  = "MdeModulePkg"
    ModuleTypes  = "DXE_RUNTIME_DRIVER"
    Token = "SMM_SUPPORT" "=" "1"
End
