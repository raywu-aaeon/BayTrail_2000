	Name  = SmmCore_SUPPORT
	Value  = 1
	TokenType = Boolean
	TargetEQU = Yes
	TargetMAK = Yes
	Master = Yes
	Help  = "Main switch to enable SmmCore support in Project"
End

INFComponent
    Name  = "PiSmmCore"
