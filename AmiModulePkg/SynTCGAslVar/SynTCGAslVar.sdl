INFComponent
    Name  = "SynTCGAslVar"
    File  = "SynTCGAslVarDxe.inf"
    Package  = "AmiModulePkg"
    ModuleTypes  = "DXE_DRIVER"
End

INFComponent
    Name  = "SynTCGAslVar"
    File  = "SynTCGAslVarSMM.inf"
    Package  = "AmiModulePkg"
    ModuleTypes  = "DXE_SMM_DRIVER"
    Token = "USING_SMI" "=" "1"
End

INFComponent
    Name  = "SynTCGAml"
    File  = "SynTcgAml.inf"
    Package  = "AmiModulePkg"
    ModuleTypes  = "DXE_DRIVER"
End



