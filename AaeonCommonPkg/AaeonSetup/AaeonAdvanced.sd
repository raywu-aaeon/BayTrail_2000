					FAN2MODE3AUTODUTYT3
					FAN2MODE3AUTODUTYT4
					FAN2MODE3AUTODUTYF1
					FAN2MODE3AUTODUTYF2
					FAN2MODE3AUTODUTYF3
					FAN2MODE3AUTODUTYF4
					FAN2MODE3AUTODUTYF5
				endif;
				#endif //F81866_SMF2_SUPPORT == 1

			endform;
        	#endif //F81866_SMF_SUPPORT == 1
