    	help    = STRING_TOKEN(STR_MISCELLANEOUS_CONFIG_FORM_HELP);
		// USB Configuration
    	goto USB_CONFIG_FORM_ID,
        prompt  = STRING_TOKEN(STR_USB_CONFIGURATION),
